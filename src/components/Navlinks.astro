---
import type { Link } from "./Navlink.astro";
import Navlink from "./Navlink.astro";

const links: Link[] = [
  {
    label: "Guides",
    href: "/guides",
    exactMatch: false,
  },
  {
    label: "Gear",
    href: "/gear",
    exactMatch: false,
  },
];
---

{
  links.map(({ label, href, exactMatch }) => (
    <Navlink label={label} href={href} exactMatch={exactMatch} />
  ))
}
